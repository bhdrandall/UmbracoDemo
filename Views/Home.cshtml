@using Umbraco.Cms.Web.Common.PublishedModels;
@using UmbracoDemo.ViewModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage<HomeViewModel>
@{
	ViewData["Title"] = Model.PageTitle;
}

@await Html.PartialAsync("~/Views/Partials/_Banner.cshtml", new BannerViewModel
{
	BannerTitle = Model.BannerTitle,
	BannerDescription = Model.BannerDescription,
	BannerImage = Model.BannerImage
})

@Html.Partial("_SpaceflightCards", Model.SpaceflightApiResponse)

@Html.GetBlockListHtml(Model.Blocks)
